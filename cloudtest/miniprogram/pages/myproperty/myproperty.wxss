/* pages/myproperty/myproperty.wxss */
.page{
  width: 100%;
  height: 100%;
  position: absolute;
}
.row{
  display: flex;
}
.navigator{
  display: flex;
  flex-direction: column;
  width:100%;
  border-radius: 30rpx;
}
.btn_icon{
  width:70rpx;
  height:70rpx;
  align-self: center;
}
.btn_text{
  align-self: center;
  font-weight: normal;
  font-size:30rpx;
}

.propertyList{
  display:flex;
  flex-direction: column;
  margin-top: 20rpx; 
  height: 85%; 
  background-color: rgba(175, 175, 175, 0.068) ;
}
.scroll_height{
  height:100%;
}
.scroll_height2{
  height:92%;
}
.item{
  display: flex;
  margin: 10rpx;
  background-color: rgba(195, 228, 236, 0.295) ;
  border-radius: 30rpx;
}
.list_imagesize{
  display:flex;
  margin:25rpx;
  width: 230rpx;
  height: 230rpx;
  border:5rpx dashed #47ADE7;
  justify-content: center;         
  align-items:center;              
}
.list_image{
  width: 230rpx;
  height: 230rpx;
}
.des_detail{
  display: flex;
  width:100%;
  flex-direction: column;
}
.link_text{
  color: rgba(68, 117, 221, 0.959);
  font-size: smaller;
  margin: 20rpx;
}
.btn_box{
  width: 100%;
  height: 10%;
  display: flex;
  align-items: center;
  justify-content: center; 
  background-color: rgba(230, 230, 230, 0.11);
}
.navigate_addObject{
  display: flex;
  align-items: center;
  justify-content: center;  
  background-color:  rgba(71, 172, 231, 0.664);
  border-radius: 60rpx;
  color:#fff;
  font-weight: bold;
  width:40%;
  height:80%;
}
.navigate_addObject2{
  display: flex;
  align-items: center;
  justify-content: center;  
  background-color:  rgba(162, 196, 236, 0.178);
  border-radius: 60rpx;
  color:rgb(101, 180, 226);
  font-weight: bold;
  width:40%;
  height:65%;
}